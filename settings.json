  "editor.tokenColorCustomizations": {
    "[Cyberpunk 2021]": {
      "textMateRules": [
        {
          "scope": "variable",
          "settings": {
            "foreground": "#FFFFFF"
          }
        },
        {
          "scope": "comment",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "keyword.control",
          "settings": {
            "foreground": "#00B0FF",
            "fontStyle": ""
          }
        },
        {
          "scope": "string.quoted.double",
          "settings": {
            "foreground": "#00B0FF"
          }
        },
        {
          "scope": "storage.modifier",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "storage.type",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "keyword",
          "settings": {
            "fontStyle": ""
          }
        },
        {
          "scope": "entity.name.function",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "string",
          "settings": {
            "foreground": "#C953D4"
          }
        },
        {
          "scope": "variable.parameter",
          "settings": {
            "foreground": "#CC5D9D"
          }
        },
        {
          "scope": "variable.other.property",
          "settings": {
            "foreground": "#FF00CC"
          }
        },
        {
          "scope": "constant.language",
          "settings": {
            "foreground": "#FF00CC"
          }
        }
      ],
    }
